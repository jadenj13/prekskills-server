{
  "name": "prekskills",
  "version": "1.0.0",
  "description": "prekskills",
  "main": "handler.js",
  "scripts": {
    "build": "run-s format:fix fix lint tsc",
    "tsc": "node node_modules/typescript/bin/tsc",
    "dev": "sls offline start",
    "deploy:dev": "sls deploy --stage dev --aws-profile personal",
    "deploy:stage": "sls deploy --stage staging --aws-profile personal",
    "deploy:prod": "sls deploy --stage prod -s production --aws-profile personal",
    "format:fix": "pretty-quick --staged",
    "format:check": "prettier --config ./.prettierrc --list-different \"app/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:all": "prettier --config ./.prettierrc --write \"app/**/*{.ts,.js,.json,.css,.scss}\"",
    "lint": "node_modules/.bin/eslint .  --ext .ts,.js",
    "fix": "node_modules/.bin/eslint . --ext .ts,.js --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s build"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.4.5",
    "aws-sdk": "^2.658.0",
    "bcrypt": "^4.0.1",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.8.10",
    "serverless-http": "^2.3.1",
    "uuid": "^7.0.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.2",
    "@types/express-jwt": "0.0.40",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.7.0",
    "@types/node": "^13.5.1",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.0",
    "husky": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "serverless-offline": "^5.12.1",
    "serverless-plugin-typescript": "^1.1.9",
    "typescript": "^3.7.5"
  }
}
